{
  "name": "@react-fluent-edit/core",
  "version": "1.0.3",
  "license": "MIT",
  "description": "Slate based editor powered by plugins",
  "type": "module",
  "sideEffects": false,
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sodenn/react-fluent-edit.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/sodenn/react-fluent-edit",
  "author": {
    "name": "sodenn",
    "email": "mail@sodenn.dev"
  },
  "scripts": {
    "build": "yarn clean && yarn build:esm & yarn build:cjs && yarn fixup",
    "clean": "rimraf lib",
    "fixup": "../../scripts/patch-package-json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json"
  },
  "dependencies": {
    "remark-parse": "10.0.1",
    "remark-slate-transformer": "0.7.0",
    "remark-stringify": "10.0.2",
    "unified": "10.1.2"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "slate": "^0.81.3",
    "slate-history": "^0.66.0",
    "slate-react": "^0.81.0"
  }
}
