{
  "name": "@react-fluent-edit/mui",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Bindings for using React FluentEdit with MUI",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "files": [
    "lib",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sodenn/react-fluent-edit.git",
    "directory": "packages/mentions"
  },
  "homepage": "https://github.com/sodenn/react-fluent-edit",
  "author": {
    "name": "sodenn",
    "email": "mail@sodenn.dev"
  },
  "scripts": {
    "build": "yarn clean && yarn build:esm & yarn build:cjs && yarn fixup",
    "clean": "rimraf lib",
    "fixup": "../../scripts/patch-package-json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json"
  },
  "devDependencies": {
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "@mui/material": "5.9.2",
    "@react-fluent-edit/core": "workspace:*",
    "@react-fluent-edit/mentions": "workspace:*"
  }
}
