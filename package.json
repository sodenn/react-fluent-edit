{
  "private": true,
  "packageManager": "yarn@3.2.3",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach run build",
    "clean": "yarn workspaces foreach run clean",
    "version:pre": "yarn workspaces foreach --no-private version ${PKG_VERSION}",
    "publish": "yarn workspaces foreach --no-private run publish",
    "test": "jest",
    "lint": "npx eslint packages/**/src/**",
    "fmt": "prettier --check packages/**/src/**",
    "test:e2e": "playwright test -c playwright-ct.config.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@playwright/experimental-ct-react": "1.26.0",
    "@playwright/test": "1.26.0",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-interactions": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/builder-webpack4": "6.5.12",
    "@storybook/manager-webpack4": "6.5.12",
    "@storybook/react": "6.5.12",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.0.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "babel-jest": "29.0.3",
    "babel-loader": "8.2.5",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.31.8",
    "fast-glob": "3.2.12",
    "fs-extra": "10.1.0",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "3.0.2",
    "slate": "0.82.1",
    "slate-history": "0.66.0",
    "slate-react": "0.83.0",
    "ts-jest": "29.0.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  }
}
